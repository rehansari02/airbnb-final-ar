
<% if(sucess && sucess.length){ %>
<div class="row">
  <div class="alert alert-success alert-dismissible fade show mx-auto text-center" role="alert" style="width: fit-content;">
   <%= sucess %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
</div>
<% } %>

<% if(errorMsg && errorMsg.length){ %>
<div class="row">
  <div class="alert alert-danger alert-dismissible fade show mx-auto text-center" role="alert" style="width: fit-content;">
   <%= errorMsg %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
</div>
<% } %>


